    "def isHappy(n):\n",
    "    n = str(n)\n",
    "    sum = 0\n",
    "    for i in n:\n",
    "        sum += int(i)**2\n",
    "    if sum == 1:\n",
    "        return True\n",
    "    elif int(n)>50:\n",
    "        return False\n",
    "    else:\n",
    "        return isHappy(sum)    \n"
